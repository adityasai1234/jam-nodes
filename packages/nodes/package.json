  "devDependencies": {
    "typescript": "^5.7.0",
    "zod": "^3.24.0"
  },
  "keywords": [
    "workflow",
    "nodes",
    "conditional",
    "automation"
  ],
  "author": "Jam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wespreadjam/jam-nodes.git",
    "directory": "packages/nodes"
  }
